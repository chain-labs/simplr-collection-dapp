{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "./src/*"
      ],
      "pages/*": [
        "./pages/*"
      ],
      "containers/*": [
        "./src/containers/*"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "svgs/*": [
        "./src/svgs/*"
      ],
      "styleguide/*": [
        "./src/styleguide/*"
      ],
      "images/*": [
        "./public/static/images/*"
      ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/src/style"
, "src/containers/create/components/signDoc", "src/containers/create/components/PaymentPage/index.tsxt"  ],
  "exclude": [
    "node_modules",
    ".next",
    "out"
  ]
}
